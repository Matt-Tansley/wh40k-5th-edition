      <profiles>
        <profile name="Howling Griffons - Chaplain Dreadnought Titus" typeId="828a-679b-1a4d-9511" typeName="Walker" hidden="false" id="7263-ac3d-3e73-6e8e">
          <characteristics>
            <characteristic name="WS" typeId="088d-2068-bd5a-62fc">5</characteristic>
            <characteristic name="BS" typeId="1fbd-cea7-aaee-6448">5</characteristic>
            <characteristic name="S" typeId="794d-acb1-e52a-414c">6(10)</characteristic>
            <characteristic name="Front" typeId="d53c-c046-cfa8-dbc4">12</characteristic>
            <characteristic name="Side" typeId="31b8-be05-a169-eec6">12</characteristic>

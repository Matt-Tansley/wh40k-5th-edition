    <selectionEntry type="unit" import="true" name="Astral Claws - Lugft Huron" hidden="false" id="a429-e5e4-b2cf-7efe">
      <profiles>
        <profile name="Astral Claws - Lugft Huron" typeId="618c-4b59-7703-dea7" typeName="Unit" hidden="false" id="fbdb-cf3b-c33f-dcfb">
          <characteristics>
            <characteristic name="WS" typeId="1a67-35be-8c50-5f48">6</characteristic>
            <characteristic name="BS" typeId="a833-788f-22a7-be67">5</characteristic>
            <characteristic name="S" typeId="3a56-2050-a9e7-ddf4">4</characteristic>
